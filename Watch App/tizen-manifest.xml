	<ui-application appid="org.tizen.example.TizenNoXaml"
					exec="WorkoutHistoryRecorder.WatchApp.dll"
					type="dotnet"
					multiple="false"
					taskmanage="true"
					nodisplay="false"
					launch_mode="single"
                    >
